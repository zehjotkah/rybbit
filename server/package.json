{
  "name": "rybbit-backend",
  "version": "1.5.1",
  "description": "Rybbit backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsc && node dist/index.js",
    "build": "tsc && npm run build:analytics",
    "build:analytics": "cd src/analytics-script && node build.js",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:pull": "drizzle-kit pull --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:check": "drizzle-kit check --config=drizzle.config.ts",
    "knip": "knip"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@axiomhq/pino": "^1.3.1",
    "@clickhouse/client": "1.11.1",
    "@fastify/cors": "11.0.1",
    "@fastify/one-line-logger": "2.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "8.1.1",
    "@maxmind/geoip2-node": "6.1.0",
    "@mongodb-js/zstd": "^2.0.1",
    "@rybbit/shared": "file:../shared",
    "better-auth": "1.3.1",
    "bullmq": "^5.56.5",
    "dotenv": "16.5.0",
    "drizzle-orm": "0.43.1",
    "fastify": "5.3.3",
    "fastify-better-auth": "1.1.0",
    "ip-address": "^10.0.1",
    "isbot": "^5.1.28",
    "luxon": "3.6.1",
    "node-cache": "^5.1.2",
    "node-cron": "4.0.6",
    "pg": "8.16.0",
    "postgres": "3.4.5",
    "psl": "^1.15.0",
    "resend": "^4.5.1",
    "rrweb": "^2.0.0-alpha.11",
    "sqlstring": "2.3.3",
    "stripe": "18.1.0",
    "twilio": "^5.8.0",
    "ua-parser-js": "2.0.3",
    "undici": "^7.9.0",
    "web-vitals": "^5.0.3",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/luxon": "3.6.2",
    "@types/node": "^20.14.8",
    "@types/pg": "^8.15.1",
    "@types/psl": "^1.1.3",
    "@types/sqlstring": "2.3.2",
    "drizzle-kit": "0.31.1",
    "esbuild": "^0.25.5",
    "jsdom": "^26.1.0",
    "knip": "^5.60.2",
    "pino-pretty": "^13.1.1",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  }
}
